<!doctype html>
<html>
  <head>
    <title><%= title %></title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="css/<%= cssFile %>?v=<%= version %>">
    <script type="text/javascript">window.appVersion='<%= version %>';</script>
  </head>
  <body>
    <div class="main">
      <header class="header">Fixed Header</header>
      <div class="chat">
        <script type="text/template" id="chatListItem">
          <div class="chat-list-item">
            <figure>
              <div class="chat-list-item-photo"></div>
            </figure>
            <div class="chat-list-item-text">Test Text</div>
            <div class="chat-list-item-actions">
              <button>Reply</button>
            </div>
            <div class="chat-list"></div>
          </div>
        </script>
        <script type="text/template" id="chatList">
          <div class="chat-list"></div>
        </script>
        <div id="chatView"></div>
      </div>
    </div>
    <script type="text/javascript" src="js/<%= jsFile %>?v=<%= version %>"></script>
  </body>
</html>